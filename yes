UserInputService.InputBegan:Connect(function(Input,yes)
	if Input.UserInputType==Enum.UserInputType.Keyboard then
		if Input.KeyCode==Enum.KeyCode.Semicolon or Input.UserInputType==Enum.KeyCode.Colon then
			if Enabled==false then
				Enabled=true
				Kevin.Enabled=Enabled
			elseif Enabled==true then
				Enabled=false
				Kevin.Enabled=Enabled
			end
		end
	end
end)

local function CULR_fake_script() -- ClearFog.LocalScript 
	local script=Instance.new('LocalScript',ClearFog)
	script.Parent.MouseButton1Click:Connect(function()
		Lighting.FogEnd=math.huge
		Lighting.FogStart=math.huge
		Lighting.FogColor=Color3.new(1,1,1)
	end)
end
coroutine.wrap(CULR_fake_script)()
local function XRIEQM_fake_script() -- ClearLighting.LocalScript 
	local script=Instance.new('LocalScript',ClearLighting)
	script.Parent.MouseButton1Click:Connect(function()
		Lighting.FogEnd=math.huge
		Lighting.FogStart=math.huge
		Lighting.FogColor=Color3.new(1,1,1)
		Lighting.TimeOfDay="12:00:00"
		Lighting.ClockTime=12
		Lighting.GeographicLatitude=12
		Lighting.ExposureCompensation=0
	end)
end
coroutine.wrap(XRIEQM_fake_script)()
local function UPVL_fake_script() -- ClearLighting_2.LocalScript 
